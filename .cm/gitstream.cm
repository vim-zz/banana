# -*- mode: yaml -*-
manifest:
  version: 1.0

automations:
  add_team_as_reviewer_1:
    if:
      - true
    run:
      - action: add-comment@v1
        args:
          comment: |
            unchecked? {{ pr.description | includes(regex=r/^- \[\s+\]/) | some }}

  labales:
    if:
      - true
    run:
      - action: add-labels@v1
        args:
          labels: abc,def,gfe,banana


