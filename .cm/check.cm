
manifest:
  version: 1.0

automations:
  plugin_test:
    if:
      - true
    run:
      - action: add-comment@v1
        args:
          comment: |
            newFiles: {{ newFiles }}
            parseConfig: {{ parseConfig }}
            stuff: {{ stuff }}


parseConfig: {{ [source.diff.files, repo] | lookForDependabotStuff("pom\.xml$", env.CODEOWNERS) }}
